#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i, a, sum;
   for(int i = 1; i <= 10; i++){
    printf("%d enter integer: \n", i);
    scanf("%d", &a);
    if(i <= 5){
      sum = sum + a;
    }

     // printf("sum equals to %d", sum);
   }
   printf("sum equals to %d", sum);
    return 0;
}
