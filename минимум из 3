#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a, b, c;
    printf("Enter 3 integers: ");
    scanf("%d %d %d", &a, &b, &c);
    if(a < b & a < c){
        printf("min %d", a);
    }
    if(b < a & b < c){
        printf("min %d", b);
    }
    if(c < a & c < b){
        printf("min %d", c);
    }
    return 0;
}
